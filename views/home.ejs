<html>
<head>
  <link rel="stylesheet" href="./css/style.css">
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
</head>
  <body>
    <%- include("partials/header") %>
    <div class="container">
      <div class="tasks" id="task" >
        <div class="header">
          <h1>Your Notes</h1>
        
        </div>
    
      </div>
      <div class="add-task" id="add-task">
        <h1>Add Notes</h1>
        <form class="my-form" method="post" action="/notes">
          <div class="form-group">
            <input placeholder="note" type="text" name="note" autocomplete="off" id="input" wrap="">
            <input type="submit" value="Add" class="btn btn-primary" id="btn">
          </div>
        </form>
      </div>
    </div>
    
    <script src="/js/script.js"></script>
  </body>
</html>
  
